{
  "env": {
    "browser": true,
    "es6": true
  },
  "settings": {
    "react": {
      "version": "16.6.0"
    }
  },
  "extends": ["prettier", "eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "tabWidth": 2,
        "singleQuote": true,
        "printWidth": 120,
        "trailingComma": "es5",
        "object-property-newline": "true",
        "array-element-newline": "true",
        "react/prop-types": [0, {}],
        "react/jsx-indent": [2, 2],
        "comma-dangle": [2, "only-multiline"],
        "linebreak-style": [2, "unix"],
        "indent": [2, 2]
      }
    ],
    "no-console": "off",
    "no-debugger": "off"

  }
}
